{
  "es6-css-minify.minifyOnSave": true,
  "crane.saveCache": false,
  "files.associations": {
    "*.html": "twig"
  },
  "emmet.includeLanguages": {
    "twig": "html"
  },
  "auto-close-tag.SublimeText3Mode": true,
  "auto-close-tag.enableAutoCloseTag": true,
  "auto-close-tag.enableAutoCloseSelfClosingTag": true,
  "auto-close-tag.activationOnLanguage": [
    "*"
  ],
  "auto-close-tag.fullMode": true,
  "highlight-matching-tag.enabled": true,
  "highlight-matching-tag.showPath": true,
  "highlight-matching-tag.showRuler": true,
  "highlight-matching-tag.highlightSelfClosing": true,
  //"highlight-matching-tag.styles":	{ opening: { name: { underline: 'yellow' } } }	
  "highlight-matching-tag.styles": {
    "opening": {
      "left": {
        "underline": "yellow"
      },
      "right": {
        "surround": "#155FFA"
      },
      "name": {
        "highlight": "rgba(180, 20, 80, 0.3)"
      }
    },
    "closing": {
      "full": {
        "custom": {
          "dark": {
            "borderWidth": "0 0 1px 0",
            "borderColor": "white",
            "borderStyle": "solid",
            "borderRadius": "4px",
            "right": "10px"
          },
          "light": {
            "borderWidth": "0 0 1px 0",
            "borderColor": "brown",
            "borderStyle": "solid",
            "borderRadius": "4px",
            "right": "10px"
          }
        }
      }
    }
  },
  "tslint.enable": false,
  "tslint.jsEnabl": false,
  "css.remoteStyleSheets": [
    "https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/css/bootstrap.min.css"
  ],
